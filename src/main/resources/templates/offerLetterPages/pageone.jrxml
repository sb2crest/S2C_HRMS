                </textElement>
                <textFieldExpression><![CDATA["Dear "+"<font color='#000000'><style isBold='true'>"+$P{offer}.getFullName()+"</style></font>"+","]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="47" y="134" width="152" height="24" uuid="2e431626-0dfa-4670-9b17-a3310a8d5bd7"/>
                <textElement verticalAlignment="Middle">
                    <font fontName="Times New Roman" size="13" isBold="true"/>
                </textElement>
